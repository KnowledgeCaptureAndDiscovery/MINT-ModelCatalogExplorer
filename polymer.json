{
  "entrypoint": "index.html",
  "shell": "src/mint-explorer-app.html",
  "fragments": [
    "src/model-configuration.html",
    "src/model-descriptions.js",
    "src/model-search.html",
    "src/my-icons.html",
    "src/not-found.html",
    "src/queries.js",
    "src/shared-styles.html",
    "src/variable-presentation.html",
    "src/view-model.html",
    "src/my-about.html",
    "src/variable-search.html",
    "src/loading-screen.html",
    "src/font-awesome.html"
